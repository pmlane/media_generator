# Google AI API key for Gemini image generation
GOOGLE_AI_API_KEY=

# Image generation model (default: gemini-3-pro-image-preview)
GEMINI_IMAGE_MODEL=gemini-3-pro-image-preview

# Estimated cost per generation in cents
GEMINI_COST_PER_IMAGE_CENTS=4

# OpenAI API key for GPT Image generation
OPENAI_API_KEY=

# OpenAI image model (default: gpt-image-1)
OPENAI_IMAGE_MODEL=gpt-image-1

# OpenAI image quality: low, medium, high (default: medium)
OPENAI_IMAGE_QUALITY=medium

# OpenAI cost per image in cents (default: 4 = medium quality)
OPENAI_COST_PER_IMAGE_CENTS=4

# Daily budget cap in cents (default: 500 = $5.00)
DAILY_BUDGET_CENTS=500

# Rate limit: max concurrent requests
MAX_CONCURRENT_REQUESTS=1

# Rate limit: minimum delay between requests in ms
REQUEST_DELAY_MS=500

# Provider timeout in ms (default: 60000 = 60s)
PROVIDER_TIMEOUT_MS=60000

# Max retries on transient failure
MAX_RETRIES=2
